{
  "name": "node-polyglot",
  "version": "2.0.0",
  "description": "Give your JavaScript the ability to speak many languages.",
  "main": "index.js",
  "scripts": {
    "pretest": "npm run --silent lint",
    "test": "npm run --silent tests-only",
    "tests-only": "NODE_ICU_DATA=node_modules/full-icu mocha test/*.js --reporter spec",
    "lint": "eslint *.js test/*.js",
    "docs": "docco -o docs/ index.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/airbnb/polyglot.js.git"
  },
  "keywords": [
    "i18n",
    "internationalization",
    "internationalisation",
    "translation",
    "interpolation",
    "translate",
    "polyglot"
  ],
  "author": "Spike Brehm <spike@airbnb.com>",
  "dependencies": {
    "for-each": "^0.3.2",
    "has": "^1.0.1",
    "object.assign": "^4.0.4",
    "warning": "^2.1.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "docco": "^0.7.0",
    "eslint": "^3.7.1",
    "eslint-config-airbnb-base": "^8.0.0",
    "eslint-plugin-import": "^1.16.0",
    "full-icu": "^1.0.3",
    "mocha": "^3.1.2",
    "should": "^11.1.1",
    "uglify-js": "^2.7.3"
  },
  "license": "BSD-2-Clause"
}
