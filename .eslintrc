{
  "root": true,

  "extends": "airbnb-base/legacy",

  "parserOptions": {
    "ecmaFeatures": {
        "modules": true
    },
    "sourceType": "module"
  },

  "rules": {
    "func-names": 0,
    "one-var": 0,
    "one-var-declaration-per-line": 0,
    "no-use-before-define": 1,
    "vars-on-top": 0,
    "yoda": 1,
  }
}
